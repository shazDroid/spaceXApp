<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="441">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@id/imageViewEnlarge"
            motion:clickAction="toggle" />
    </Transition>

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="441">
        <KeyFrameSet>
        </KeyFrameSet>
        <OnClick motion:targetId="@id/imageViewShrink"
            motion:clickAction="toggle" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="600dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="match_parent"
            motion:layout_constraintTop_toBottomOf="@+id/appCompatTextView"
            android:layout_marginLeft="16dp"
            android:id="@+id/displayImageView"
            android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/horizontalRecyclerView"
            android:layout_marginStart="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView15"
            motion:layout_constraintWidth_percent="0.9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView14"
            motion:layout_constraintRight_toRightOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/textView14"
            android:layout_marginTop="8dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView4"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            motion:layout_constraintTop_toBottomOf="@+id/appCompatTextView"
            android:layout_marginTop="16dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView4"
            android:layout_marginTop="24dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView6"
            motion:layout_constraintEnd_toEndOf="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView5"
            android:layout_marginTop="4dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView7"
            motion:layout_constraintEnd_toEndOf="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView6"
            android:layout_marginTop="22dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView8"
            motion:layout_constraintEnd_toEndOf="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView7"
            android:layout_marginTop="4dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView9"
            motion:layout_constraintEnd_toEndOf="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView8"
            android:layout_marginTop="22dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView10"
            motion:layout_constraintEnd_toEndOf="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView9"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView11"
            motion:layout_constraintEnd_toEndOf="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView10"
            android:layout_marginTop="22dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/diameterTv"
            motion:layout_constraintEnd_toEndOf="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView11"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView13"
            motion:layout_constraintEnd_toEndOf="@+id/diameterTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/diameterTv"
            android:layout_marginTop="20dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/textView12"
            motion:layout_constraintEnd_toEndOf="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView13"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/imageViewEnlarge"
            motion:layout_constraintEnd_toEndOf="@+id/displayImageView"
            android:layout_width="48dp"
            android:layout_height="48dp"
            motion:layout_constraintBottom_toBottomOf="@+id/displayImageView"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="16dp"
            android:translationZ="1dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageViewEnlarge"
            motion:layout_constraintEnd_toEndOf="@+id/displayImageView"
            android:layout_width="48dp"
            android:layout_height="48dp"
            motion:layout_constraintBottom_toBottomOf="@+id/displayImageView"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="16dp"
            android:translationZ="1dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/imageViewShrink"
            motion:layout_constraintEnd_toEndOf="@+id/displayImageView"
            android:layout_width="48dp"
            android:layout_height="48dp"
            motion:layout_constraintBottom_toBottomOf="@+id/displayImageView"
            android:layout_marginBottom="16dp"
            android:visibility="visible"
            android:layout_marginEnd="16dp"
            android:translationZ="1dp" />
    </ConstraintSet>
</MotionScene>